<WTEngineDisplay>
	<EngineDisplayPage>
        <ID>ENGINE</ID>
		<Title>Engine</Title>
		<Node>EngineDisplay</Node>
	</EngineDisplayPage>
	<EngineDisplayPage>
        <ID>SYSTEM</ID>
		<Title>System</Title>
		<Node>EngineDisplaySystem</Node>
	</EngineDisplayPage>
	<EngineDisplayPage>
        <ID>FUEL</ID>
		<Title>Fuel</Title>
		<Node>EngineDisplayFuel</Node>
		<Button>
            <Text>DEC FUEL</Text>
        </Button>
        <Button>
            <Text>INC FUEL</Text>
        </Button>
		<Button>
            <Text>RST FUEL</Text>
        </Button>
	</EngineDisplayPage>
	
	
	<EngineDisplaySystem>
	
		<Gauge><!--Load62_Master-->
			<Type>DoubleVertical</Type>
			<Style>
				<Height>60</Height>
				<TextIncrement>1</TextIncrement>
				<ValuePos>Begining</ValuePos>
				<ForceTextColor>white</ForceTextColor>
			</Style>
			<ID>Piston_LoadGauge</ID>
			<Title></Title>
			<Unit></Unit>
			<Minimum>0</Minimum>
			<Maximum>100</Maximum>
			<Value>
				<If>
				<Condition>
				<Simvar name="RECIP ENG ENGINE MASTER SWITCH:1" unit="Bool"/>
				</Condition>
				<Then>			
				<Max>
					<Min>
						<Multiply>
							<Divide>
								<Multiply>
									<Simvar name="ENG TORQUE:1" unit="Foot pounds"/>
									<Divide>
										<Simvar name="GENERAL ENG RPM:1" unit="rpm"/>
										<Constant>5252</Constant>
									</Divide>
									<Constant>550</Constant>
								</Multiply>
									<Constant>99000</Constant><!--to fix fuel pump-->
							</Divide>
							<Constant>100</Constant>
						</Multiply>
						<Constant>100</Constant>
					</Min>
					<Constant>0</Constant>
				</Max>
				</Then>
				<Else>
					<Constant>-100</Constant>
				</Else>
				</If>						
			</Value>
			<Value2>
				<If>
				<Condition>
				<Simvar name="RECIP ENG ENGINE MASTER SWITCH:2" unit="Bool"/>
				</Condition>
				<Then>			
				<Max>
					<Min>
						<Multiply>
							<Divide>
								<Multiply>
									<Simvar name="ENG TORQUE:2" unit="Foot pounds"/>
									<Divide>
										<Simvar name="GENERAL ENG RPM:2" unit="rpm"/>
										<Constant>5252</Constant>
									</Divide>
									<Constant>550</Constant>
								</Multiply>
									<Constant>99000</Constant><!--to fix fuel pump-->
							</Divide>
							<Constant>100</Constant>
						</Multiply>
						<Constant>100</Constant>
					</Min>
					<Constant>0</Constant>
				</Max>
				</Then>
				<Else>
					<Constant>-100</Constant>
				</Else>
				</If>
			</Value2>
			<ColorZone>
				<Color>white</Color>
				<Begin>0</Begin>
				<End>95.25</End>
			</ColorZone>
			<ColorZone>
				<Color>green</Color>
				<Begin>0</Begin>
				<End>95.25</End>
			</ColorZone>
			<ColorZone>
				<Color>yellow</Color>
				<Begin>95</Begin>
				<End>100</End>
			</ColorZone>			
			<GraduationLength text="True">20</GraduationLength>
			<BeginText>Load%</BeginText>
		</Gauge>
		
		<Gauge><!--RPM62_Master-->
			<Type>DoubleVertical</Type>
			<Style>
				<Height>60</Height>
				<TextIncrement>10</TextIncrement>
				<ForceTextColor>white</ForceTextColor>
			</Style>
			<ID>Piston_RPMGauge</ID>
			<Title></Title>
			<Unit></Unit>
			<Minimum>0</Minimum>
			<Maximum>3000</Maximum>
			<Value>
			<If>
				<Condition>
					<Simvar name="RECIP ENG ENGINE MASTER SWITCH:1" unit="Bool"/>
				</Condition>
			<Then>
				<Simvar name="PROP RPM:1" unit="rpm"/>
			</Then>
			<Else>
				<Constant>-3000</Constant>
			</Else>
			</If>
			</Value>
			<Value2>
			<If>
				<Condition>
					<Simvar name="RECIP ENG ENGINE MASTER SWITCH:2" unit="Bool"/>
				</Condition>
			<Then>
				<Simvar name="PROP RPM:2" unit="rpm"/>
			</Then>
			<Else>
				<Constant>-3000</Constant>
			</Else>
			</If>
			</Value2>
			<ColorZone>
				<Color>white</Color>
				<Begin>0</Begin>
				<End>2205</End>
			</ColorZone>
			<ColorZone>
				<Color>green</Color>
				<Begin>0</Begin>
				<End>2205</End>
			</ColorZone>
			<ColorZone>
				<Color>yellow</Color>
				<Begin>2205</Begin>
				<End>2305</End>
			</ColorZone>
			<ColorZone>
				<Color>red</Color>
				<Begin>2305</Begin>
				<End>3000</End>
			</ColorZone>
			<GraduationLength text="True">600</GraduationLength>
			<BeginText>RPM</BeginText>
		</Gauge>
		
		<Header><!--Electircal-->
			<Text>Electrical</Text>
		</Header>
		
		<Gauge><!--Volts-->
			<Type>DoubleHorizontal</Type>
			<ID>Voltage_Gauge</ID>
			<Title></Title>
			<Unit>Volts</Unit>
			<CursorText>L</CursorText>
			<CursorText2>R</CursorText2>
			<Minimum>23</Minimum>
			<Maximum>34</Maximum>
			<Style>
					<ForceTextColor>white</ForceTextColor>
					<SizePercent>100</SizePercent>
			</Style>
			<Value>
					<Simvar name="L:XMLVAR_MAIN_BUS_VOLTAGE_1" unit="volts"/>				
			</Value>
			<Value2>
					<Simvar name="L:XMLVAR_MAIN_BUS_VOLTAGE_2" unit="volts"/>	
			</Value2>
			<ColorZone>
				<Color>red</Color>
				<Begin>23</Begin>
				<End>24.1</End>
			</ColorZone>
			<ColorZone>
				<Color>yellow</Color>
				<Begin>24.1</Begin>
				<End>25</End>
			</ColorZone>
			<ColorZone>
				<Color>green</Color>
				<Begin>25</Begin>
				<End>30</End>
			</ColorZone>
			<ColorZone>
				<Color>yellow</Color>
				<Begin>30</Begin>
				<End>32</End>
			</ColorZone>
			<ColorZone>
				<Color>red</Color>
				<Begin>32</Begin>
				<End>34</End>
			</ColorZone>
			<BeginText>23</BeginText>
			<EndText>34</EndText>
			<YellowBlink>
				<Or>
					<Greater>
						<Simvar name="L:XMLVAR_MAIN_BUS_VOLTAGE_1" unit="volts"/>
						<Constant>30</Constant>
					</Greater>
					<Lower>
						<Simvar name="L:XMLVAR_MAIN_BUS_VOLTAGE_1" unit="volts"/>
						<Constant>25</Constant>
					</Lower>
				</Or>
			</YellowBlink>
			<RedBlink>
				<Or>
					<Greater>
						<Simvar name="L:XMLVAR_MAIN_BUS_VOLTAGE_1" unit="volts"/>
						<Constant>32</Constant>
					</Greater>
					<Lower>
						<Simvar name="L:XMLVAR_MAIN_BUS_VOLTAGE_1" unit="volts"/>
						<Constant>24.1</Constant>
					</Lower>
				</Or>
			</RedBlink>
		</Gauge>
		
