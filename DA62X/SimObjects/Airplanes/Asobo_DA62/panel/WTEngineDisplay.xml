				</Substract>
				</ToFixed>
			</Left>
			<Center>°C</Center>
			<Right>
				<ToFixed precision="0">
				<Simvar name="L:XMLVAR_FUEL_TEMPERATURE" unit="celsius"/>
				</ToFixed>
			</Right>
		</Text>
		
		<Header><!--FuelCalc-->
			<Text>Fuel Calc</Text>
		</Header>
		
		<Text><!--GalRem-->
			<Left>Gal Rem</Left>			
			<Right>
				<ToFixed precision="1">
					<Simvar name="FUEL TOTAL QUANTITY:1" unit="gallon"/>
				</ToFixed>
			</Right>
		</Text>
		
		<Text><!--GalUsed-->
			<Left>Gal Used</Left>			
			<Right>
				<ToFixed precision="1">
					<Substract>
						<MaxSinceStart>
							<Simvar name="FUEL TOTAL QUANTITY:1" unit="gallon"/>
						</MaxSinceStart>
						<Simvar name="FUEL TOTAL QUANTITY:1" unit="gallon"/>
					</Substract>
				</ToFixed>
			</Right>
		</Text>
		
		<Text><!--Endr-->
			<Left>Endur</Left>			
			<Right>
				<Duration>
					<Multiply>					
						<Divide>
								<Simvar name="FUEL TOTAL QUANTITY:1" unit="gallon"/>
							<Add>
								<Simvar name="ENG FUEL FLOW GPH:1" unit="gallons per hour"/>
								<Simvar name="ENG FUEL FLOW GPH:2" unit="gallons per hour"/>
							</Add>
						</Divide>
						<Constant>3600</Constant>						
					</Multiply>
				</Duration>
			</Right>
		</Text>
		
		<Text><!--Range-->
			<Left>Range NM</Left>			
			<Right>
				<If>
                    <Condition>
                        <Greater>
						<Add>
                            <Simvar name="ENG FUEL FLOW GPH:1" unit="gallons per hour"/>
							<Simvar name="ENG FUEL FLOW GPH:2" unit="gallons per hour"/>
						</Add>
                            <Constant>2</Constant>
                        </Greater>
                    </Condition>
				<Then>				
					<ToFixed precision="0">
						<Multiply>
							<Simvar name="GROUND VELOCITY:1" unit="knots"/>
							<Divide>
								<Simvar name="FUEL TOTAL QUANTITY:1" unit="gallon"/>							
								<Add>
									<Simvar name="ENG FUEL FLOW GPH:1" unit="gallons per hour"/>
									<Simvar name="ENG FUEL FLOW GPH:2" unit="gallons per hour"/>
								</Add>
							</Divide>
						</Multiply>
				</ToFixed>
				</Then>
				<Else>
						<Constant>X</Constant>
				</Else>
				</If>
			</Right>
		</Text>
		
		<Text></Text>
		
		<Text><!--TTltimetext-->
		<Center>Ttl Time In Svc</Center>
		</Text>
		
		<Header><!--Line-->
			<Text></Text>
		</Header>
		
		<Text><!--Hrs-->
			<Center>
				<ToFixed precision="1">
					<Simvar name="GENERAL ENG ELAPSED TIME:1" unit="hour"/>
				</ToFixed>
			</Center>
			<Right>Hrs</Right>
		</Text>
		
	</EngineDisplayFuel>

