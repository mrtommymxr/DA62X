		<Header><!--Engine-->
			<Text>Engine</Text>
		</Header>
	
		<Gauge><!--Gearbox_XML-->
			<Type>DoubleHorizontal</Type>
			<ID>Piston_CoolantTempGauge</ID>
			<Title>Gearbox</Title>
			<Unit></Unit>
			<CursorText>L</CursorText>
			<CursorText2>R</CursorText2>
			<Minimum>-35</Minimum>
			<Maximum>145</Maximum>
			<Style>
				<ForceTextColor>white</ForceTextColor>
				<ValuePos>Top</ValuePos>
				<SizePercent>100</SizePercent>
			</Style>
			<Value>
					<Simvar name="L:XMLVAR_GBOX_TEMPERATURE_1" unit="celsius"/>
			</Value>
			<Value2>
					<Simvar name="L:XMLVAR_GBOX_TEMPERATURE_2" unit="celsius"/>
			</Value2>
			<ColorZone>
				<Color>red</Color>
				<Begin>-35</Begin>
				<End>-30</End>
			</ColorZone>
			<ColorZone>
				<Color>yellow</Color>
				<Begin>-30</Begin>
				<End>35</End>
			</ColorZone>
			<ColorZone>
				<Color>green</Color>
				<Begin>35</Begin>
				<End>115</End>
			</ColorZone>
			<ColorZone>
				<Color>yellow</Color>
				<Begin>115</Begin>
				<End>120</End>
			</ColorZone>
			<ColorZone>
				<Color>red</Color>
				<Begin>120</Begin>
				<End>145</End>
			</ColorZone>
			<BeginText>-35</BeginText>
			<EndText>145</EndText>
			<YellowBlink>
				<Or>
					<Greater>
						<Multiply>
						<Add>
							<Simvar name="RECIP ENG RADIATOR TEMPERATURE:1" unit="celsius"/>
							<Simvar name="GENERAL ENG OIL TEMPERATURE:1" unit="celsius"/>
						</Add>
							<Constant>0.5</Constant>
						</Multiply>
						<Constant>115</Constant>
					</Greater>
					<Lower>
						<Multiply>
						<Add>
							<Simvar name="RECIP ENG RADIATOR TEMPERATURE:1" unit="celsius"/>
							<Simvar name="GENERAL ENG OIL TEMPERATURE:1" unit="celsius"/>
						</Add>
							<Constant>0.5</Constant>
						</Multiply>
						<Constant>35</Constant>
					</Lower>
				</Or>
			</YellowBlink>
			<RedBlink>
				<Or>
					<Greater>
						<Multiply>
						<Add>
							<Simvar name="RECIP ENG RADIATOR TEMPERATURE:1" unit="celsius"/>
							<Simvar name="GENERAL ENG OIL TEMPERATURE:1" unit="celsius"/>
						</Add>
						<Constant>0.5</Constant>
						</Multiply>
						<Constant>120</Constant>
					</Greater>
					<Lower>
						<Multiply>
						<Add>
							<Simvar name="RECIP ENG RADIATOR TEMPERATURE:1" unit="celsius"/>
							<Simvar name="GENERAL ENG OIL TEMPERATURE:1" unit="celsius"/>
						</Add>
						<Constant>0.5</Constant>
						</Multiply>
						<Constant>-30</Constant>
					</Lower>
				</Or>
			</RedBlink>
		</Gauge>
		
